            rdfs:subClassOf <https://w3id.org/italia/onto/l0/Agent> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasAlternativeIdentifier ;
                              owl:allValuesFrom :Identifier
                            ] ,
