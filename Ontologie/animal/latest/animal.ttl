###  https://w3id.org/leo/onto/animal/AlternativeIdentifier
:AlternativeIdentifier rdf:type owl:Class ;
                       rdfs:subClassOf l0:Characteristic ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :assignedBy ;
                                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass cov:Organization
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty l0:identifier ;
                                         owl:cardinality "1"^^xsd:nonNegativeInteger
                                       ] ;
                       rdfs:comment "Alternative identifier used for livestock species, productive orientation, production target code lists when other organizations assign a code which is different from the AIA one."@en ,
                                    "Identificativo alternativo utilizzato per alcune code list del dominio zootecnico dove per la specie, per l'indirizzo produttivo e per l'orientamento produttivo, altre organizzazioni assegnano codici identificativi a questi concetti diversi dal codice AIA"@it ;
                       rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                       rdfs:label "Alternative Identifier"@en ,
                                  "Identificativo Alternativo"@it ;
                       owl:versionInfo "provvisorio - inserito nella seconda versione dell'ontologia"@it ,
                                       "unstable - included in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/Breed
:Breed rdf:type owl:Class ;
       rdfs:subClassOf l0:Concept ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :isBreedOfFarmAnimal ;
                         owl:allValuesFrom :FarmAnimal
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty l0:name ;
                         owl:someValuesFrom rdfs:Literal
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :AIABreedCode ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                       ] ;
       rdfs:comment "It represents the breed of the farm animal."@en ,
                    "Rappresenta la razza dell'animale allevato"@it ;
       rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
       rdfs:label "Breed"@en ,
                  "Razza"@it ;
       owl:versionInfo "stabile"@it ,
                       "stable"@en .


###  https://w3id.org/leo/onto/animal/DeadWeight
:DeadWeight rdf:type owl:Class ;
            rdfs:subClassOf :Weight ;
            owl:disjointWith :LiveWeight ;
            rdfs:comment "Il peso morto dell'animale d'allevamento ovvero il peso dopo la data di macellazione."@it ,
                         "The deadweight of the farm animal or the weight after the date of slaughter."@en ;
            rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
            rdfs:label "Deadweight"@en ,
                       "Peso Morto"@it ;
            owl:versionInfo "provvisorio - aggiunto nella seconda versione dell'ontologia"@it ,
                            "unstable - added in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/FarmAnimal
:FarmAnimal rdf:type owl:Class ;
            rdfs:subClassOf l0:Agent ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasAlternativeIdentifier ;
                              owl:allValuesFrom :AlternativeIdentifier
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasUniqueAnimalNumberInTime ;
                              owl:allValuesFrom :UniqueAnimalNumberInTime
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasSex ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :Sex
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasBreed ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :Breed
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasDestination ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :FarmAnimalDestination
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasFather ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :FarmAnimal
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasGeneticMother ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :FarmAnimal
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :isOfSpecies ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :LivestockSpecies
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty ale:birthDate ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:date
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty ale:registrationDate ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:date
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :shortAnimalName ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :uniqueAnimalNumber ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty ale:disposalDate ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:date
                            ] ;
            rdfs:comment "It represents the farm animal."@en ,
                         "Rappresenta il soggetto o l'animale da allevamento."@it ;
            rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
            rdfs:label "Animale di Allevamento"@it ,
                       "Farm Animal"@en ;
            owl:versionInfo "stabile"@it ,
                            "stable"@en .


###  https://w3id.org/leo/onto/animal/FarmAnimalDestination
:FarmAnimalDestination rdf:type owl:Class ;
                       rdfs:subClassOf l0:Characteristic ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :isDestinationOf ;
                                         owl:allValuesFrom :FarmAnimal
                                       ] ;
                       rdfs:comment "It represents the managerial decision which the breeder makes when the animal is born about his willingness to raise the animal for breeding or slaughter."@en ,
                                    "Rappresenta la decisione manageriale che l’allevatore prende al momento della nascita dell’animale circa la sua volontà di allevare l’animale da riproduzione o destinarlo alla macellazione."@it ;
                       rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                       rdfs:label "Destinazione dell'Animale di Allevamento"@it ,
                                  "Farm Animal Destination"@en ;
                       owl:versionInfo "stabile"@it ,
                                       "stable"@en .


###  https://w3id.org/leo/onto/animal/LiveWeight
:LiveWeight rdf:type owl:Class ;
            rdfs:subClassOf :Weight ;
            rdfs:comment "Il peso vivo dell'animale d'allevamento."@it ,
                         "The live weight of the farm animal."@en ;
            rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
            rdfs:label "Live Weight"@en ,
                       "Peso Vivo"@it ;
            owl:versionInfo "provvisorio - aggiunto nella seconda versione dell'ontologia"@it ,
                            "unstable - added in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/LivestockSpecies
:LivestockSpecies rdf:type owl:Class ;
                  rdfs:subClassOf l0:Concept ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasAlternativeIdentifier ;
                                    owl:allValuesFrom :AlternativeIdentifier
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :isSpeciesOf ;
                                    owl:allValuesFrom :FarmAnimal
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty l0:name ;
                                    owl:someValuesFrom rdfs:Literal
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :AIASpeciesCode ;
                                    owl:cardinality "1"^^xsd:nonNegativeInteger
                                  ] ;
                  rdfs:comment "It represents the zootechnical species. By species it means: cattle, buffaloes, goats, sheep, pigs. The scientific names of the species are not used."@en ,
                               "Rappresenta la specie zootecnica. Per specie si intende: bovini, bufali, capre, ovini, suini. Non vengono usati i nomi scientifici delle specie."@it ;
                  rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                  rdfs:label "Livestock Species"@en ,
                             "Specie Zootecnica"@it ;
                  owl:versionInfo "stabile - aggiornata nella seconda versione da Specie a Specie Zootecnica"@it ,
                                  "stable - update from Species to Livestock Species"@en .


###  https://w3id.org/leo/onto/animal/ProductionTarget
:ProductionTarget rdf:type owl:Class ;
                  rdfs:subClassOf l0:Characteristic ,
                                  l0:Concept ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasAlternativeIdentifier ;
                                    owl:allValuesFrom :AlternativeIdentifier
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty l0:name ;
                                    owl:someValuesFrom rdfs:Literal
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :AIAPTCode ;
                                    owl:cardinality "1"^^xsd:nonNegativeInteger
                                  ] ;
                  rdfs:comment "Indirizzo Produttivo per il quale l’animale viene allevato. E’ indicato all’ingresso dell’animale nell’allevamento"@it ,
                               "Production target for which the animal is farmed. It is indicated during the displacement in entry of the animal"@en ;
                  rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                  rdfs:label "Indirizzo Produttivo"@it ,
                             "Production Target"@en ;
                  owl:versionInfo "stabile - aggiunto nella seconda versione dell'ontologia"@it ,
                                  "stable - added in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/ProductiveOrientation
:ProductiveOrientation rdf:type owl:Class ;
                       rdfs:subClassOf l0:Characteristic ,
                                       l0:Concept ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasAlternativeIdentifier ;
                                         owl:allValuesFrom :AlternativeIdentifier
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty l0:name ;
                                         owl:someValuesFrom rdfs:Literal
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :AIAPOCode ;
                                         owl:cardinality "1"^^xsd:nonNegativeInteger
                                       ] ;
                       rdfs:comment "Orientamento Produttivo della Razza"@it ,
                                    "The productive orientation of the breed."@en ;
                       rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                       rdfs:label "Orientamento Produttivo"@it ,
                                  "Productive Orientation"@en ;
                       owl:versionInfo "stabile - aggiunto nella seconda versione dell'ontologia"@it ,
                                       "stable - added in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/Sex
:Sex rdf:type owl:Class ;
     rdfs:subClassOf l0:Characteristic ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :isSexOf ;
                       owl:allValuesFrom :FarmAnimal
                     ] ;
     rdfs:comment "It represents the sex of the farm animal. Its values are F:female and M:Male"@en ,
                  "Rappresenta il sesso dell'animale e può assumere solo due valori F: Femmina e M:Maschio"@it ;
     rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
     rdfs:label "Sesso"@it ,
                "Sex"@en ;
     owl:versionInfo "stabile"@it ,
                     "stable"@en .


###  https://w3id.org/leo/onto/animal/UniqueAnimalNumberInTime
:UniqueAnimalNumberInTime rdf:type owl:Class ;
                          rdfs:subClassOf <https://w3id.org/italia/onto/TI/TimeIndexedEvent> ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :prevUniqueAnimalNumber ;
                                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                            owl:onClass :UniqueAnimalNumberInTime
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :uniqueAnimalNumber ;
                                            owl:cardinality "1"^^xsd:nonNegativeInteger
                                          ] ;
                          rdfs:comment "Questa classe rappresenta la situazione del numero unico degli animali d'allevamento che cambiano nel tempo. La situazione è abbastanza rara poiché l'animale che perde il suo numero unico (matricola) può ri-acquisirlo. Tuttavia, vi è un 10% di casi in cui il numero unico può cambiare nel tempo. Questa classe permette di tenere traccia di questi cambiamenti, compresa la relazione con il numero unico precedente."@it ,
                                       "This class represents the situation of the unique animal number that changes over time. The situation is quite rare since the animal that looses its unique number can re-acquire it. However, there are a 10% of cases in which the unique number may change over time. This class allows one to keep track of these changes including the relation with the previous unique number."@en ;
                          rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                          rdfs:label "Numero Unico Animale nel Tempo"@it ,
                                     "Unique Animal Number in Time"@en ;
                          owl:versionInfo "provvisorio - aggiunto nella seconda versione delle ontologie"@it ,
                                          "unstable - added in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/Weight
:Weight rdf:type owl:Class ;
        rdfs:subClassOf <https://w3id.org/italia/onto/MU/Value> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :methodWeightDetection ;
                          owl:allValuesFrom rdfs:Literal
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty ale:detectionDate ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange [ rdf:type rdfs:Datatype ;
                                            owl:unionOf ( xsd:date
                                                          xsd:dateTime
                                                        )
                                          ]
                        ] ;
        rdfs:comment "Peso dell'animale"@it ,
                     "Weight of the farm animal"@en ;
        rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
        rdfs:label "Peso"@it ,
                   "Weight"@en ;
        owl:versionInfo "provvisorio - aggiunto nella seconda versione dell'ontologia"@it ,
                        "unstable - added in the second version of the ontology"@en .


