#    Object Properties
#################################################################

###  https://w3id.org/leo/onto/animal/assignedBy
:assignedBy rdf:type owl:ObjectProperty ;
            rdfs:domain :AlternativeIdentifier ;
            rdfs:range cov:Organization ;
            rdfs:comment "Property that links the alternative identifier to the organization responsable for assigning it."@en ,
                         "Proprietà che consente di collegare il codice alternativo all'organizzazione che lo assegna."@it ;
            rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
            rdfs:label "assegnato da"@it ,
                       "assigned by"@en ;
            owl:versionInfo "provvisorio - introdotto nella seconda versione dell'ontologia"@it ,
                            "unstable - included in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/hasAlternativeIdentifier
:hasAlternativeIdentifier rdf:type owl:ObjectProperty ;
                          rdfs:domain [ rdf:type owl:Class ;
                                        owl:unionOf ( :FarmAnimal
                                                      :LivestockSpecies
                                                      :ProductionTarget
                                                      :ProductiveOrientation
                                                    )
                                      ] ;
                          rdfs:range :AlternativeIdentifier ;
                          rdfs:comment "Property that allows one to specify an alternative identifier for the livestock species, productive orientation, farm animal and production target."@en ,
                                       "Proprietà che consente di specificare un codice alternato alle specie zootecniche, all'orientamento produttivo, agli animali d'allevamento e all'indirizzo produttivo."@it ;
                          rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                          rdfs:label "ha identificativo alternativo"@it ,
                                     "has alternative identifier"@en ;
                          owl:versionInfo "provvisorio - aggiunto nella seconda versione dell'ontologia"@it ,
                                          "unstable - added in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/hasBreed
:hasBreed rdf:type owl:ObjectProperty ;
          owl:inverseOf :isBreedOfFarmAnimal ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( ale:Birth
                                      ale:DisplacementInEntry
                                      ale:Identification
                                      :FarmAnimal
                                    )
                      ] ;
          rdfs:range :Breed ;
          rdfs:comment "Property that represents the belonging relationship between a farm animal and its breed."@en ,
                       "Proprietà che rappresenta la relazione di appartenenza dell'animale alla sua razza."@it ;
          rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
          rdfs:label "appartiene a"@it ,
                     "belongs to"@en ;
          owl:versionInfo "stabile"@en ,
                          "stable"@en .


###  https://w3id.org/leo/onto/animal/hasDestination
:hasDestination rdf:type owl:ObjectProperty ;
                owl:inverseOf :isDestinationOf ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( ale:Birth
                                            ale:DisplacementInEntry
                                            ale:Identification
                                            :FarmAnimal
                                          )
                            ] ;
                rdfs:range :FarmAnimalDestination ;
                rdfs:comment "Property that links the farm animal to its destination (life or slaughterhouse)."@en ,
                             "Proprietà che collega l'animale alla sua destinazione (vita, macello)"@it ;
                rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                rdfs:label "ha destinazione"@it ,
                           "has destination"@en ;
                owl:versionInfo "stabile"@it ,
                                "stable"@en .


###  https://w3id.org/leo/onto/animal/hasFather
:hasFather rdf:type owl:ObjectProperty ;
           rdfs:domain :FarmAnimal ;
           rdfs:range :FarmAnimal ;
           rdfs:comment "Property that allows one to specify the father of the farm animal."@en ,
                        "Proprietà che consente di specificare il padre dell'animale."@it ;
           rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
           rdfs:label "ha padre"@it ,
                      "has father"@en ;
           owl:versionInfo "stabile"@it ,
                           "stable"@en .


###  https://w3id.org/leo/onto/animal/hasGeneticMother
:hasGeneticMother rdf:type owl:ObjectProperty ;
                  rdfs:domain :FarmAnimal ;
                  rdfs:range :FarmAnimal ;
                  rdfs:comment "Property that allows one to specify the mother who brings in the genetic inheritance"@en ,
                               "Proprietà che consente di specificare la madre che apporta il patrimonio genetico"@it ;
                  rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                  rdfs:label "ha madre genetica"@it ,
                             "has genetic mother"@en ;
                  owl:versionInfo "stabile"@it ,
                                  "stable"@en .


###  https://w3id.org/leo/onto/animal/hasProductionTarget
:hasProductionTarget rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isProductionTargetOf ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( ale:DisplacementInEntry
                                                 :FarmAnimal
                                                 :LivestockSpecies
                                               )
                                 ] ;
                     rdfs:range :ProductionTarget ;
                     rdfs:comment "Property that links the farm animal (and thus the event of displacement in entry) and the livestock species to their production target"@en ,
                                  "Proprietà che collega l'animale (e quindi l'evento di spostamento dell'animale in entrata all'allevamento) o la specie zootecnica all'indirizzo produttivo di riferimento."@it ;
                     rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                     rdfs:label "ha indirizzo produttivo"@it ,
                                "has production target"@en ;
                     owl:versionInfo "stabile - aggiunto nella seconda versione dell'ontologia"@it ,
                                     "stable - added in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/hasProductiveOrientation
:hasProductiveOrientation rdf:type owl:ObjectProperty ;
                          owl:inverseOf :isProductiveOrientationOf ;
                          rdfs:domain :Breed ;
                          rdfs:range :ProductiveOrientation ;
                          rdfs:comment "Property that links the breed to its productive orientation"@en ,
                                       "Proprietà che collega la razza al proprio orientamento produttivo"@it ;
                          rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                          rdfs:label "ha orientamento produttivo"@it ,
                                     "has productive orientation"@en ;
                          owl:versionInfo "stabile - aggiunto nella seconda versione dell'ontologia"@it ,
                                          "stable - added in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/hasSex
:hasSex rdf:type owl:ObjectProperty ;
        owl:inverseOf :isSexOf ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( ale:Birth
                                    ale:DisplacementInEntry
                                    ale:Identification
                                    :FarmAnimal
                                  )
                    ] ;
        rdfs:range :Sex ;
        rdfs:comment "Property that links the farm animal with its sex."@en ,
                     "Proprietà che collega l'animale al suo sesso (femmina, maschio)"@it ;
        rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
        rdfs:label "ha sesso"@it ,
                   "has sex"@en ;
        owl:versionInfo "stabile"@it ,
                        "stable"@en .


###  https://w3id.org/leo/onto/animal/hasUniqueAnimalNumberInTime
:hasUniqueAnimalNumberInTime rdf:type owl:ObjectProperty ;
                             rdfs:domain :FarmAnimal ;
                             rdfs:range :UniqueAnimalNumberInTime ;
                             rdfs:comment "Property that links the farm animal to the situation where the unique number associated with it may change over time."@en ,
                                          "Proprietà che lega l'animale d'allevamento alla situazione in cui il numero unico ad esso associato può variare nel tempo."@it ;
                             rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                             rdfs:label "ha numero unico animale nel tempo"@it ,
                                        "has unique animal number in time"@en ;
                             owl:versionInfo "provvisorio - aggiunto nella seconda versione dell'ontologia"@it ,
                                             "unstable - added in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/hasWeight
:hasWeight rdf:type owl:ObjectProperty ;
           rdfs:domain owl:Thing ;
           rdfs:range :Weight ;
           rdfs:comment "Property that links the farm animal to its weight."@en ,
                        "Proprietà che lega l'animale al suo peso."@it ;
           rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
           rdfs:label "ha peso"@it ,
                      "has weight"@en ;
           owl:versionInfo "provvisorio - aggiunto nela seconda versione dell'ontologia"@it ,
                           "unstable - added in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/isBreedOfFarmAnimal
:isBreedOfFarmAnimal rdf:type owl:ObjectProperty ;
                     rdfs:domain :Breed ;
                     rdfs:range [ rdf:type owl:Class ;
                                  owl:unionOf ( ale:Birth
                                                ale:DisplacementInEntry
                                                ale:Identification
                                                :FarmAnimal
                                              )
                                ] ;
                     rdfs:comment "Inverse property of belongs to that links a breed to the farm animal"@en ,
                                  "Proprietà inversa di \"appartiene a\" che collega la razza all'animale."@it ;
                     rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                     rdfs:label "is breed of farm animal"@en ,
                                "è razza animale di allevamento"@it ;
                     owl:versionInfo "stabile"@it ,
                                     "stable"@en .


###  https://w3id.org/leo/onto/animal/isDestinationOf
:isDestinationOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :FarmAnimalDestination ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( ale:Birth
                                            ale:DisplacementInEntry
                                            ale:Identification
                                            :FarmAnimal
                                          )
                            ] ;
                 rdfs:comment "Inverse property of \"has destination\" that links a destination to the farm animal"@en ,
                              "Proprietà inversa di \"ha destinazione\" che collega la destinazione con l'animale."@it ;
                 rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                 rdfs:label "is destination of"@en ,
                            "è destinazione di"@it ;
                 owl:versionInfo "stabile"@it ,
                                 "stable"@en .


###  https://w3id.org/leo/onto/animal/isOfSpecies
:isOfSpecies rdf:type owl:ObjectProperty ;
             owl:inverseOf :isSpeciesOf ;
             rdfs:domain :FarmAnimal ;
             rdfs:range :LivestockSpecies ;
             rdfs:comment "Property that allows one to link the farm animal to its livestock species."@en ,
                          "Proprietà che consente di collegare l'animale alla sua specie zootecnica"@it ;
             rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
             rdfs:label "is of species"@en ,
                        "è di specie"@it ;
             owl:versionInfo "stabile"@it ,
                             "stable"@en .


###  https://w3id.org/leo/onto/animal/isProductionTargetOf
:isProductionTargetOf rdf:type owl:ObjectProperty ;
                      rdfs:domain :ProductionTarget ;
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( :FarmAnimal
                                                 :LivestockSpecies
                                               )
                                 ] ;
                      rdfs:comment "Inverse property of \"has production target\" that links the production target to the farm animal or livestock species."@en ,
                                   "Proprietà inversa di \"ha indirizzo produttivo\" che lega l'indirizzo produttivo all'animale o alla specie zootecnica"@it ;
                      rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                      rdfs:label "is production target of"@en ,
                                 "è indirizzo produttivo di"@it ;
                      owl:versionInfo "stabile - aggiunto nella seconda versione dell'ontologia"@it ,
                                      "stable - added in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/isProductiveOrientationOf
:isProductiveOrientationOf rdf:type owl:ObjectProperty ;
                           rdfs:domain :ProductiveOrientation ;
                           rdfs:range :Breed ;
                           rdfs:comment "Inverse property of \"has productive orientation\" that links the productive orientation to the breed"@en ,
                                        "Proprietà inversa di \"ha orientamento produttivo\" che lega l'orientamento produttivo alla razza"@it ;
                           rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                           rdfs:label "is productive orientation of"@en ,
                                      "è orientamento produttivo di"@it ;
                           owl:versionInfo "stabile"@it ,
                                           "stable"@en .


###  https://w3id.org/leo/onto/animal/isSexOf
:isSexOf rdf:type owl:ObjectProperty ;
         rdfs:domain :Sex ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( ale:Birth
                                    ale:DisplacementInEntry
                                    ale:Identification
                                    :FarmAnimal
                                  )
                    ] ;
         rdfs:comment "Inverse property of \"has sex\" that links the sex to the farm animal"@en ,
                      "Proprietà inversa di \"ha sesso\" che lega il sesso all'animale da allevamento"@it ;
         rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
         rdfs:label "is sex of"@en ,
                    "è sesso di"@it ;
         owl:versionInfo "stabile"@it ,
                         "stable"@en .


###  https://w3id.org/leo/onto/animal/isSpeciesOf
:isSpeciesOf rdf:type owl:ObjectProperty ;
             rdfs:domain :LivestockSpecies ;
             rdfs:range :FarmAnimal ;
             rdfs:comment "Inverse property of \"is of species\" that links the livestock species to the farm animal."@en ,
                          "Proprietà inversa di \"è di specie\" che consente di collegare la specie zootecnica all'animale"@it ;
             rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
             rdfs:label "is species of"@en ,
                        "è specie di"@en ;
             owl:versionInfo "stabile"@it ,
                             "stable"@en .


###  https://w3id.org/leo/onto/animal/isWithin
:isWithin rdf:type owl:ObjectProperty ;
          rdfs:domain :Breed ;
          rdfs:range :LivestockSpecies ;
          rdfs:comment "Property that links the breed to the livestock species"@en ,
                       "Proprietà che lega la razza alla specie zootecnica"@it ;
          rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
          rdfs:label "is within"@en ,
                     "rientra in"@it ;
          owl:versionInfo "stabile"@it ,
                          "stable"@en .


###  https://w3id.org/leo/onto/animal/prevUniqueAnimalNumber
:prevUniqueAnimalNumber rdf:type owl:ObjectProperty ;
                        rdfs:domain :UniqueAnimalNumberInTime ;
                        rdfs:range :UniqueAnimalNumberInTime ;
                        rdfs:comment "Property that allows one to keep track of the previous unique number of a farm animal in the situation in which the unique number may change over time."@en ,
                                     "Proprietà che permette di tenere traccia del precedente numero unico di un animale da allevamento nella situazione in cui il numero unico può cambiare nel tempo."@it ;
                        rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                        rdfs:label "precedente numero unico animale"@it ,
                                   "previous unique animal number"@en ;
                        owl:versionInfo "provvisorio - aggiunto nella seconda versione dell'ontologia"@it ,
                                        "unstable - added in the second version of the ontology"@en .


