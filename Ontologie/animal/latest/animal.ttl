###  https://w3id.org/leo/onto/animal/methodWeightDetection
:methodWeightDetection rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf l0:description ;
                       rdfs:domain :Weight ;
                       rdfs:range rdfs:Literal ;
                       rdfs:comment "Property that links the weight to the method used to estimate it."@en ,
                                    "Proprietà che collega il peso dell'animale al metodo usato per stimarlo."@it ;
                       rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                       rdfs:label "method weight detection"@en ,
                                  "metodo di rilevazione peso"@it ;
                       owl:versionInfo "provvisorio - aggiunto nella seconda versione dell'ontologia"@it ,
                                       "unstable - added in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/shortAnimalName
:shortAnimalName rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf l0:name ;
                 rdfs:domain :FarmAnimal ;
                 rdfs:range rdfs:Literal ;
                 rdfs:comment "Name used commercially to recognise the animal (mainly used for males)"@en ,
                              "Nome utilizzato commercialmente per riconoscere l’animale (usato soprattutto per i maschi)"@it ;
                 rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                 rdfs:label "nome corto animale"@it ,
                            "short animal name"@en ;
                 owl:versionInfo "stabile - aggiunto nella seconda versione dell'ontologia"@it ,
                                 "stable - added in the second version of the ontology"@en .


###  https://w3id.org/leo/onto/animal/uniqueAnimalNumber
:uniqueAnimalNumber rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf l0:identifier ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( ale:Birth
                                                ale:DisplacementInEntry
                                                ale:Identification
                                                :FarmAnimal
                                                :UniqueAnimalNumberInTime
                                              )
                                ] ;
                    rdfs:range rdfs:Literal ;
                    rdfs:comment "Codice identificativo univoco dell'animale."@it ,
                                 "Unique code that identifies the farm animal."@en ;
                    rdfs:isDefinedBy <https://w3id.org/leo/onto/animal> ;
                    rdfs:label "numero univoco animale"@it ,
                               "unique animal number"@en ;
                    owl:versionInfo "stabile"@it ,
                                    "stable"@en .


