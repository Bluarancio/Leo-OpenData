        <ADMS:hasOntologyLanguage rdf:resource="https://w3id.org/italia/onto/ADMS/OWL"/>
        <ADMS:hasSemanticAssetDistribution rdf:resource="https://w3id.org/leo/data/distribution/animal-JSON-LD"/>
        <ADMS:hasSemanticAssetDistribution rdf:resource="https://w3id.org/leo/data/distribution/animal-RDF-TURTLE"/>
        <ADMS:hasSemanticAssetDistribution rdf:resource="https://w3id.org/leo/data/distribution/animal-RDF-XML"/>
        <ADMS:hasTask rdf:resource="https://w3id.org/italia/onto/ADMS/AnnotationTask"/>
        <ADMS:hasTask rdf:resource="https://w3id.org/italia/onto/ADMS/QueryFormulationTask"/>
        <ADMS:hasTask rdf:resource="https://w3id.org/italia/onto/ADMS/SearchTask"/>
        <ADMS:officialURI rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://w3id.org/leo/onto/animal</ADMS:officialURI>
        <ADMS:prefix>animal</ADMS:prefix>
        <ADMS:semanticAssetInUse rdf:resource="https://w3id.org/leo/data/project/LEO"/>
        <ADMS:status>catalogued</ADMS:status>
        <ADMS:target>Imprese</ADMS:target>
        <ADMS:target>Pubbliche Amministrazioni</ADMS:target>
        <ADMS:type rdf:resource="https://w3id.org/italia/onto/ADMS/CoreOntology"/>
    </owl:Ontology>



