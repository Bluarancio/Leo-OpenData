    <!-- https://w3id.org/leo/onto/animal/assignedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/animal/assignedBy">
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/animal/Identifier"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/COV/Organization"/>
        <rdfs:comment xml:lang="en">Property that links the alternative identifier to the organization responsable for assigning it.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che consente di collegare il codice alternativo all&apos;organizzazione che lo assegna.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/animal"/>
        <rdfs:label xml:lang="it">assegnato da</rdfs:label>
        <rdfs:label xml:lang="en">assigned by</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile - introdotto nella seconda versione dell&apos;ontologia</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable - included in the second version of the ontology</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/animal/belongsTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/animal/belongsTo">
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/animal/LivestockSpecies"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/animal/LivestockFamily"/>
        <rdfs:comment xml:lang="en">Property used to link the  species to its family in scientific taxonomy in livestock context.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per collegare la specie alla relativa famiglia nella tassonomia scientifica in ambito zootecnico.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/animal"/>
        <rdfs:label xml:lang="it">appartiene a</rdfs:label>
        <rdfs:label xml:lang="en">belongs to</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile - introdotta nella versione 1.1 dell&apos;ontologia</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable - introduced in version 1.1 of the ontology</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/animal/hasAlternativeIdentifier -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/animal/hasAlternativeIdentifier">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/animal/Identifier"/>
        <rdfs:comment xml:lang="en">Property that allows one to specify an alternative identifier for the livestock species, farm animal and production type.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che consente di specificare un codice alternato alle specie zootecniche, agli animali d&apos;allevamento e al tipo di produzione.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/animal"/>
        <rdfs:label xml:lang="it">ha identificativo alternativo</rdfs:label>
        <rdfs:label xml:lang="en">has alternative identifier</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile - revisionato il dominio della proprietà per essere aperto a tante cose nel dominio</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable - revised the domain of the property to be open to many things in the domain</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/animal/hasAnimalDestination -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/animal/hasAnimalDestination">
        <owl:inverseOf rdf:resource="https://w3id.org/leo/onto/animal/isAnimalDestinationOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/leo/onto/ale/Birth"/>
                    <rdf:Description rdf:about="https://w3id.org/leo/onto/animal/FarmAnimal"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/animal/FarmAnimalDestination"/>
        <rdfs:comment xml:lang="en">Property that links the farm animal to its destination (life or slaughterhouse).</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che collega l&apos;animale alla sua destinazione (vita, macello)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/animal"/>
        <rdfs:label xml:lang="it">ha destinazione animale d&apos;allevamento</rdfs:label>
        <rdfs:label xml:lang="en">has animal destination</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/animal/hasAspect -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/animal/hasAspect">
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/animal/AspectDescription"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/animal/Aspect"/>
        <rdfs:comment xml:lang="en">Property linking the description of an aspect of the animal with the aspect itself.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che lega la descrizione di un carattere informativo dell&apos;animale con il carattere informativo stesso.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/animal"/>
        <rdfs:label xml:lang="it">ha carattere informativo</rdfs:label>
        <rdfs:label xml:lang="en">has aspect</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile - introdotta in versione 1.1 dell&apos;ontologia</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable - introduced in version 1.1 of the ontology</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/animal/hasAspectDescriptionCollection -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/animal/hasAspectDescriptionCollection">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/leo/onto/animal/FarmAnimal"/>
                    <rdf:Description rdf:about="https://w3id.org/leo/onto/animal/Herd"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/animal/AspectDescriptionCollection"/>
        <rdfs:comment xml:lang="en">Property that allows on to specify descriptions of different aspect of farm animals and herds</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che permette di specificare le descrizioni di diversi caratteri informativi degli animali dall&apos;allevamento e delle mandrie</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/animal"/>
        <rdfs:label xml:lang="it">ha collezione di descrizioni di caratteri informativi</rdfs:label>
        <rdfs:label xml:lang="en">has aspect description collection</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile - introdotta in versione 1.1 dell&apos;ontologia</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable - introduced in version 1.1 of the ontology</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/animal/hasBreed -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/animal/hasBreed">
        <owl:inverseOf rdf:resource="https://w3id.org/leo/onto/animal/isBreedOfFarmAnimal"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/animal/FarmAnimal"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/animal/Breed"/>
        <rdfs:comment xml:lang="en">Property that represents the belonging relationship between a farm animal and its breed.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che rappresenta la relazione di appartenenza dell&apos;animale alla sua razza.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/animal"/>
        <rdfs:label xml:lang="it">appartiene a</rdfs:label>
        <rdfs:label xml:lang="en">belongs to</rdfs:label>
        <owl:versionInfo xml:lang="en">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/animal/hasFarmAnimalStatus -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/animal/hasFarmAnimalStatus">
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/animal/FarmAnimal"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/animal/FarmAnimalStatus"/>
        <rdfs:comment xml:lang="en">Property that allows one to specify the conditions or the status of the farm animal. The value of the status are defined in the following controlled vocabulary https://w3id.org/leo/controlled-vocabulary/farm-animal-status</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che consente di specifcare le condizioni o lo stato del capo, condizioni definite nel vocabolario controllato https://w3id.org/leo/controlled-vocabulary/farm-animal-status</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/animal"/>
        <rdfs:label xml:lang="it">ha stato del capo</rdfs:label>
        <rdfs:label xml:lang="en">has farm animal status</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile - introdotta in versione 1.0 dell&apos;ontologia</owl:versionInfo>
        <owl:versionInfo xml:lang="it">stable - introduced in version 1.0 of the ontology</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/animal/hasFather -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/animal/hasFather">
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/animal/FarmAnimal"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/animal/FarmAnimal"/>
        <rdfs:comment xml:lang="en">Property that allows one to specify the father of the farm animal.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che consente di specificare il padre dell&apos;animale.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/animal"/>
        <rdfs:label xml:lang="it">ha padre</rdfs:label>
        <rdfs:label xml:lang="en">has father</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/animal/hasFatherBreed -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/animal/hasFatherBreed">
