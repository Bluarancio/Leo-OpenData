    <!-- https://w3id.org/leo/onto/co/isOwnerFarmingCompanyOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/co/isOwnerFarmingCompanyOf">
        <owl:inverseOf rdf:resource="https://w3id.org/leo/onto/co/hasOwnerFarmingCompany"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/co/LivestockCompany"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/animal/Herd"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/co"/>
        <rdfs:label xml:lang="en">is owner farming company of</rdfs:label>
        <rdfs:label xml:lang="it">è azienda proprietaria di</rdfs:label>
