        <owl:versionInfo xml:lang="en">stable - introduced in version 0.3 of the ontology</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/co/directlyFollowsCompany -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/co/directlyFollowsCompany">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/l0/directlyFollows"/>
        <owl:inverseOf rdf:resource="https://w3id.org/leo/onto/co/directlyPrecedesCompany"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/co/LivestockCompanyInTime"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/co/LivestockCompanyInTime"/>
        <rdfs:comment xml:lang="en">Property used to trace the company that cames next in the chain of transfers of animals over time.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per traccia l&apos;azienda che segue nella catena di trasferimenti degli animali nel tempo</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/co"/>
        <rdfs:label xml:lang="en">directly follows company</rdfs:label>
        <rdfs:label xml:lang="it">segue direttamente azienda</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/co/directlyPrecedesCompany -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/co/directlyPrecedesCompany">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/l0/directlyPrecedes"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/co/LivestockCompanyInTime"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/co/LivestockCompanyInTime"/>
        <rdfs:comment xml:lang="en">Inverse property of &quot;directly follows company&quot;; it identifies the company that precedes in the chain of possible transfers of animals over time.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;segue direttamente azienda&quot; ; identifica l&apos;azienda che precede nella catena di trasferimenti degli animali nel tempo</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/co"/>
        <rdfs:label xml:lang="en">directly precedes company</rdfs:label>
        <rdfs:label xml:lang="it">precede direttamente azienda</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/co/forCompany -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/co/forCompany">
        <owl:inverseOf rdf:resource="https://w3id.org/leo/onto/co/isCompanyOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/co/LivestockCompanyInTime"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/co/LivestockCompany"/>
        <rdfs:comment xml:lang="en">Property that links the situation &quot;company in time&quot; with its company</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che collega la situazione di &quot;azienda nel tempo&quot; all&apos;azienda stessa.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/co"/>
        <rdfs:label xml:lang="en">for company</rdfs:label>
        <rdfs:label xml:lang="it">per azienda</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/co/hasBirthFarmingCompany -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/co/hasBirthFarmingCompany">
        <owl:inverseOf rdf:resource="https://w3id.org/leo/onto/co/isBirthFarmingCompanyOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/animal/FarmAnimal"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/co/LivestockCompany"/>
        <rdfs:comment xml:lang="en">Property linking the animal to the company of birth or to the Italian company where the animal was born, identified by the code assigned to it by the Ministry of Health.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che collega l&apos;animale all&apos;azienda di nascita ovvero all’azienda italiana nella quale è nato l’animale identificata attraverso il codice che il ministero della sanità le ha assegnato.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/co"/>
        <rdfs:label xml:lang="it">ha azienda d&apos;allevamento di nascita</rdfs:label>
        <rdfs:label xml:lang="en">has birth farming company</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/co/hasCompanyInTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/co/hasCompanyInTime">
        <owl:inverseOf rdf:resource="https://w3id.org/leo/onto/co/isCompanyInTimeOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/animal/FarmAnimal"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/co/LivestockCompanyInTime"/>
        <rdfs:comment xml:lang="en">Property that links the animal to the possible chain of several companies that may follow/precede over time</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che collega l&apos;animale alla possibile catena di diverse aziende che si possono susseguire nel tempo</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/co"/>
        <rdfs:label xml:lang="it">ha azienda nel tempo</rdfs:label>
        <rdfs:label xml:lang="en">has company in time</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/co/hasHolder -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/co/hasHolder">
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/co/LivestockCompany"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:comment xml:lang="en">It is the property that binds the owner to the livestock holding. The owner is the person, natural or legal, who has ownership, to all effects of the
law, of the farm animals</rdfs:comment>
        <rdfs:comment xml:lang="it">È la proprietà che lega il proprietario all&apos;azienda zootecnica. Il proprietario è la persona, fisica o legale, che ha la proprietà, a tutti gli effetti di
legge, degli animali</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/co"/>
        <rdfs:label xml:lang="it">ha proprietario/a</rdfs:label>
        <rdfs:label xml:lang="en">has holder</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile - introdotta in versione 0.3 dell&apos;ontologia</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable - introduced in version 0.3 of the ontology</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/co/hasLastOwnerFarmingCompany -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/co/hasLastOwnerFarmingCompany">
        <owl:inverseOf rdf:resource="https://w3id.org/leo/onto/co/isLastOwnerFarmingCompanyOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/animal/FarmAnimal"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/co/LivestockCompany"/>
        <rdfs:comment xml:lang="en">Property that allows one to link the animal with its current (the last in chain of animal movements) owner company or the Italian company where the animal has physically passed through, identified by the code that the Ministry of Health assigns to it.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che consente di collegare dirattamente l&apos;animale con la sua attuale (cioè l&apos;ultima di una serie di passaggi dell&apos;animale tra varie aziende) azienda proprietaria ovverto l’azienda italiana nella quale è transitato fisicamente l’animale, identificata attraverso il codice che il ministero della sanità le  assegna</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/co"/>
        <rdfs:label xml:lang="it">ha ultima azienda d&apos;allevamento proprietaria</rdfs:label>
        <rdfs:label xml:lang="en">has last owner farming company</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/co/hasLivestockFarmSite -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/co/hasLivestockFarmSite">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/leo/onto/co/hasSite"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/co/LivestockFarm"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/co/LivestockFarmSite"/>
        <rdfs:comment xml:lang="en">Property linking the specific type of livestock company that is a livestock farm with its establishment/site.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che lega lo specifico tipo di azienda zootecnica che è l&apos;allevamento di bestiame con il suo insediamento</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/co"/>
        <rdfs:label xml:lang="it">ha insediamento allevamento di bestiame</rdfs:label>
        <rdfs:label xml:lang="en">has livestock farm site</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile - introdotta in versione 0.3 dell&apos;ontologia</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable - introduced in version 0.3 of the ontology</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/co/hasOwnerFarmingCompany -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/co/hasOwnerFarmingCompany">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/animal/Herd"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/co/LivestockCompany"/>
        <rdfs:comment xml:lang="en">Property that allows one to link the group of animal of a species (herd) to their owner livestock company.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che permette di collegare il gruppo di animali di una specie (mandria) alla loro azienda zootecnica proprietaria.</rdfs:comment>
