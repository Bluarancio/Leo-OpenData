    <!-- https://w3id.org/leo/onto/weather/consistsOfWO -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/weather/consistsOfWO">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/IoT/consistsOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/weather/WeatherObservationCollection"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/weather/WeatherObservation"/>
        <rdfs:comment xml:lang="en">Property that links the collection of weather observations to each single observations.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che collega la collezione di osservazioni meteorologiche a ogni singola osservazione.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/weather"/>
        <rdfs:label xml:lang="it">consiste di osservazioni meteo</rdfs:label>
        <rdfs:label xml:lang="en">consists of weather observation</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/weather/isInstallationOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/weather/isInstallationOf">
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/weather/WeatherStationInstallation"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/weather/WeatherStation"/>
        <rdfs:comment xml:lang="en">Property that allows one to link the situation concerning the installation of a weather station (within a farm zone) to the weather station itself.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che consente di collegare la situazione relativa all&apos;installazione di una centralina meteo (all&apos;interno di una zona dell&apos;allevamento) alla centralina stessa.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/weather"/>
        <rdfs:label xml:lang="en">is installation of</rdfs:label>
        <rdfs:label xml:lang="it">è installazione di</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/weather/isInstalledInArea -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/weather/isInstalledInArea">
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/weather/WeatherStationInstallation"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/livestockfarm-building-description/BuildingStructure"/>
        <rdfs:comment xml:lang="en">Property allowing one to link the situation regarding the installation of a weather station (typically within a farm) to the area of the farm where the weather station is installed. The area could be: heifer house, feeding area, etc.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che consente di collegare la situazione relativa all&apos;installazione di una centralina meteo (tipicamente all&apos;interno di un allevamento) con l&apos;area dell&apos;allevamento dove si installa. L&apos;area potrebbe essere: vitellaia, zona di alimentazione, ecc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/weather"/>
        <rdfs:label xml:lang="en">is installed in area</rdfs:label>
        <rdfs:label xml:lang="it">è installata in area</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/weather/managedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/weather/managedBy">
        <owl:inverseOf rdf:resource="https://w3id.org/leo/onto/weather/managesWeatherStation"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/weather/WeatherStation"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/COV/Organization"/>
        <rdfs:comment xml:lang="en">Property that links the weather station to the organization it manages it</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che collega la stazione meteo all&apos;organizzazione che la gestisce.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/weather"/>
        <rdfs:label xml:lang="it">gestita da</rdfs:label>
        <rdfs:label xml:lang="en">managed by</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/leo/onto/weather/managesWeatherStation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/leo/onto/weather/managesWeatherStation">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/COV/Organization"/>
        <rdfs:range rdf:resource="https://w3id.org/leo/onto/weather/WeatherStation"/>
        <rdfs:comment xml:lang="en">Inverse property of &quot;managed by&quot; that links an organization to the weather station it manages.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;gestita da&quot; che collega l&apos;organizzazione alla stazione meteo che essa gestisce.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/weather"/>
        <rdfs:label xml:lang="it">gestisce stazione meteo</rdfs:label>
        <rdfs:label xml:lang="en">manages weather station</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



