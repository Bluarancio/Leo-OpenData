    <!-- https://w3id.org/leo/onto/weather/IMEICode -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/leo/onto/weather/IMEICode">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/l0/identifier"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/weather/WeatherStation"/>
