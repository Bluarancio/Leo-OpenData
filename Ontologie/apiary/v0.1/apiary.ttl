                          owl:onClass <https://w3id.org/italia/onto/TI/TimeInterval>
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :numberOfHives ;
                          owl:maxCardinality "1"^^xsd:nonNegativeInteger
                        ] ,
