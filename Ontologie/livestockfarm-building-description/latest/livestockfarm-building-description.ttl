###  https://w3id.org/leo/onto/livestockfarm-building-description/hasAnimalHousingCharacteristic
:hasAnimalHousingCharacteristic rdf:type owl:ObjectProperty ;
                                rdfs:domain :AnimalHousingDescription ;
                                rdfs:range :AnimalHousingCharacteristic ;
                                rdfs:comment "Property used to specify the characteristics of the animal housing of a livestock farm site. These characteristics contribute to define the description of the animal house itself."@en ,
                                             "Proprietà utilizzata per specificare le caratteristiche del ricovero degli animali di un insediamento di un'azienda d'allevamento. Le caratteristiche contribuiscono a definire la descrizione del ricovero animali stesso."@it ;
                                rdfs:isDefinedBy <https://w3id.org/leo/onto/livestockfarm-building-description> ;
                                rdfs:label "ha caratteristica ricovero animali"@it ,
                                           "has animal housing characteristic"@en ;
                                owl:versionInfo "stabile"@it ,
                                                "stable"@en .


###  https://w3id.org/leo/onto/livestockfarm-building-description/hasBuildingConfiguration
:hasBuildingConfiguration rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf <https://w3id.org/italia/onto/l0/hasDescription> ;
                          rdfs:domain <https://w3id.org/leo/onto/co/LivestockFarmSite> ;
                          rdfs:range :BuildingConfiguration ;
                          rdfs:comment "La proprietà che collega il sito dell'azienda d'allevamento con la descrizione del suo insediamento (edificio)."@it ,
                                       "The property linking the site of the livestock farm with the description of its building(s)."@en ;
                          rdfs:isDefinedBy <https://w3id.org/leo/onto/livestockfarm-building-description> ;
                          rdfs:label "ha configurazione insediamento"@it ,
                                     "has building configuration"@en ;
                          owl:versionInfo "stabile"@it ,
                                          "stable"@en .


###  https://w3id.org/leo/onto/livestockfarm-building-description/includesAnimalHousingDescription
:includesAnimalHousingDescription rdf:type owl:ObjectProperty ;
                                  rdfs:domain :BuildingConfiguration ;
                                  rdfs:range :AnimalHousingDescription ;
                                  rdfs:comment "Property use to describe the animal housing building in a site of a livestock farm company."@en ,
                                               "Proprietà utilizzata per modellare la descrizione del ricovero degli animali in un insediamento dell'azienda d'allevamento."@it ;
                                  rdfs:isDefinedBy <https://w3id.org/leo/onto/livestockfarm-building-description> ;
                                  rdfs:label "include descrizione ricovero animali"@it ,
                                             "includes animal housing description"@en ;
                                  owl:versionInfo "stabile"@it ,
                                                  "stable"@en .


###  https://w3id.org/leo/onto/livestockfarm-building-description/includesBuildingStructure
:includesBuildingStructure rdf:type owl:ObjectProperty ;
                           rdfs:domain :BuildingConfiguration ;
                           rdfs:range :BuildingStructure ;
                           rdfs:comment "Property used to specify the types of structures (e.g., delivery room, milking room, etc.) available in a site of a livestock farm company."@en ,
                                        "Proprietà utilizzata per specificare le tipologie di strutture presenti (e.g., sala parto, sala mungitura, caseificio aziendale, ecc.) in un insediamento di un'azienda d'allevamento."@it ;
                           rdfs:isDefinedBy <https://w3id.org/leo/onto/livestockfarm-building-description> ;
                           rdfs:label "include struttura presente nell'insediamento"@it ,
                                      "includes building structure"@en ;
                           owl:versionInfo "stabile"@it ,
                                           "stable"@en .


###  https://w3id.org/leo/onto/livestockfarm-building-description/includesBuildingTypology
:includesBuildingTypology rdf:type owl:ObjectProperty ;
                          rdfs:domain :BuildingConfiguration ;
                          rdfs:range :BuildingTypology ;
                          rdfs:comment "Property used to specify the type of building (e.g., rural bulding) of a livestock farm company."@en ,
                                       "Proprietà utilizzata per specificare la tipologia di insediamento (e.g., fabbricato rurale, ecc.) di un'azienda d'allevamento."@it ;
                          rdfs:isDefinedBy <https://w3id.org/leo/onto/livestockfarm-building-description> ;
                          rdfs:label "include tipo di insediamento"@it ,
                                     "includes building typology"@en ;
                          owl:versionInfo "stabile"@it ,
                                          "stable"@en .


###  https://w3id.org/leo/onto/livestockfarm-building-description/includesWaterProvenance
:includesWaterProvenance rdf:type owl:ObjectProperty ;
                         rdfs:domain :BuildingConfiguration ;
                         rdfs:range :WaterProvenanceStructure ;
                         rdfs:comment "Property used to specify the type of provenance of the water in a site of a livestcok farm company"@en ,
                                      "Proprietà utilizzata per specificare il tipo di provenienza delle acque in un insediamento di un'azienda d'allevamento."@it ;
                         rdfs:isDefinedBy <https://w3id.org/leo/onto/livestockfarm-building-description> ;
                         rdfs:label "include provenienza acque"@it ,
                                    "includes water provenance"@en ;
                         owl:versionInfo "stabile"@it ,
                                         "stable"@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/leo/onto/livestockfarm-building-description/animalHousingBuildingYear
:animalHousingBuildingYear rdf:type owl:DatatypeProperty ,
                                    owl:FunctionalProperty ;
                           rdfs:domain :AnimalHousingDescription ;
                           rdfs:range xsd:gYear ;
                           rdfs:comment "Property used to identify the year when the building for animal housing has been constructed."@en ,
                                        "Proprietà utilizzata per identificare l'anno di costruzione del ricovero per animali."@it ;
                           rdfs:isDefinedBy <https://w3id.org/leo/onto/livestockfarm-building-description> ;
                           rdfs:label "animal housing building year"@en ,
                                      "anno di costruzione ricovero animali"@it ;
                           owl:versionInfo "stabile"@it ,
                                           "stable"@en .


###  https://w3id.org/leo/onto/livestockfarm-building-description/buildingCode
:buildingCode rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :BuildingConfiguration ;
              rdfs:range rdfs:Literal ;
              rdfs:comment "Il codice associato a ciascun edificio (insediamento) di un'azienda d'allevamento. Ad esempio se ci sono 2 edifici, questo codice può assumere i valori 1 e 2 per ciascun di essi."@it ,
                           "The code associated with each building of a livestock farm site. For example if there are 2 buildings, this code can take the values 1 and 2 for each of them."@en ;
              rdfs:isDefinedBy <https://w3id.org/leo/onto/livestockfarm-building-description> ;
              rdfs:label "building code"@en ,
                         "codice insediamento"@it ;
              owl:versionInfo "stabile"@it ,
                              "stable"@en .


