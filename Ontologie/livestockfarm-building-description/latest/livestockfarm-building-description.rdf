        <rdfs:comment xml:lang="en">The property linking the site of the livestock farm with the description of its building(s).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/livestockfarm-building-description"/>
        <rdfs:label xml:lang="it">ha configurazione insediamento</rdfs:label>
        <rdfs:label xml:lang="en">has building configuration</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


