        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/leo/onto/livestockfarm-building-description/buildingCode -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/leo/onto/livestockfarm-building-description/buildingCode">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/leo/onto/livestockfarm-building-description/BuildingConfiguration"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Il codice associato a ciascun edificio (insediamento) di un&apos;azienda d&apos;allevamento. Ad esempio se ci sono 2 edifici, questo codice può assumere i valori 1 e 2 per ciascun di essi.</rdfs:comment>
        <rdfs:comment xml:lang="en">The code associated with each building of a livestock farm site. For example if there are 2 buildings, this code can take the values 1 and 2 for each of them.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/leo/onto/livestockfarm-building-description"/>
        <rdfs:label xml:lang="en">building code</rdfs:label>
        <rdfs:label xml:lang="it">codice insediamento</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:DatatypeProperty>
    


