        <rdfs:label xml:lang="it">include struttura presente nell&apos;insediamento</rdfs:label>
        <rdfs:label xml:lang="en">includes building structure</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


