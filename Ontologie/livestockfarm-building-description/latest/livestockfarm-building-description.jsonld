    {
      "@id": "https://w3id.org/leo/onto/livestockfarm-building-description/AnimalHousingCharacteristic",
      "@type": "owl:Class",
      "rdfs:comment": [
        {
          "@language": "it",
          "@value": "La caratteristica attraverso cui viene descritto il ricovero per gli animali. Esempi includono: presenza del camino centrale, numero di lati chiusi, altezza colmo, ecc."
        },
